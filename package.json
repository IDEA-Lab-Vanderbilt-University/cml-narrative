		"daisyui": "^3.2.1",
		"html5-qrcode": "^2.3.8",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^2.8.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
