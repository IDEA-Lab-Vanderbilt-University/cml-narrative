		"@zxing/library": "^0.20.0",
		"intro.js": "^7.0.1",
		"jspdf": "^2.5.1",
		"lodash": "^4.17.21",
		"papaparse": "^5.4.1",
		"pdfkit": "^0.14.0",
		"qrcode": "^1.5.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"svelte-awesome-color-picker": "^2.2.2",
		"svelte-dnd-action": "^0.9.50",
		"svelte-file-dropzone": "^2.0.4",
		"svelte-portal": "^2.2.0",
