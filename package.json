{
	"name": "cml-narrative",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev --port 5000 --host 0.0.0.0",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check .",
		"preview": "vite preview --port 4000",
		"start": "HOST=0.0.0.0 PORT=5000 node ./build/index.js"
	},
	"type": "module",
	"dependencies": {
		"@excalidraw/excalidraw": "0.17.6",
		"@macfja/svelte-persistent-store": "^2.4.1",
		"@popperjs/core": "^2.11.8",
		"@zxing/library": "^0.21.3",
		"intro.js": "^7.2.0",
		"jspdf": "^2.5.1",
		"lodash": "^4.17.21",
		"papaparse": "^5.4.1",
		"qrcode": "^1.5.4",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"svelte-confetti": "1.4.0",
		"svelte-dnd-action": "^0.9.50",
		"svelte-file-dropzone": "2.0.8",
		"svelte-preprocess-react": "0.17.1",
		"svelte-simple-modal": "^1.6.2",
		"vite": "4.5.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.3",
		"@sveltejs/kit": "1.30.4",
		"@types/papaparse": "^5.3.14",
		"@types/qrcode": "^1.5.5",
		"@types/react": "18.3.5",
		"autoprefixer": "^10.4.20",
		"daisyui": "4.12.10",
		"html5-qrcode": "^2.3.8",
		"postcss": "8.4.45",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"prettier-plugin-tailwindcss": "0.6.6",
		"svelte": "4.2.19",
		"svelte-check": "4.0.1",
		"tailwindcss": "3.4.10",
		"tslib": "2.7.0",
		"typescript": "5.6.2"
	}
}
